{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitHelpers": false,
    "sourceMap": true
  },
  "files": [
    "./app/ts/app.ts",
    "./app/ts/ChatExampleData.ts",
    "./app/ts/components/ChatNavBar.ts",
    "./app/ts/components/ChatThreads.ts",
    "./app/ts/components/ChatWindow.ts",
    "./app/ts/models.ts",
    "./app/ts/services/_scratch.ts",
    "./app/ts/services/MessagesService.ts",
    "./app/ts/services/services.ts",
    "./app/ts/services/ThreadsService.ts",
    "./app/ts/services/UserService.ts",
    "./app/ts/util/FromNowPipe.ts",
    "./app/ts/util/util.ts",
    "./app/ts/util/uuid.ts",
    "./app/ts/vendor.ts",
    "./test/app.spec.ts",
    "./test/calculator.ts",
    "./test/services/MessagesService.spec.ts",
    "./test/services/ThreadsService.spec.ts"
  ],
  "filesGlob": [
    "./app/**/*.ts",
    "./test/**/*.ts",
    "!./node_modules/**/*.ts"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": true
  }
}
